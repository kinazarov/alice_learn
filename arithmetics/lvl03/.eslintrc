{
   "env": {
       "browser": true,
       "es6": true
   },
   "extends": "eslint:recommended",
   "parserOptions": {
       "ecmaVersion": 2018,
       "sourceType": "module"
   },
   "rules": {
       "semi": [
           "error",
           "always"
       ]
   },
   "globals": {
       "JSWapiException": true,
       "jsWapiException": true,
       "JSWapiObject": true,
       "jsWapiObject":true,
       "jsWapi": true,
       "apiCore" :true,
       "apiRequest": true,
       "apiResponse": true
   }
}
